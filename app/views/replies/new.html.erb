<% provide(:title, 'Respostas') %>
<h1>Cadastrar</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for([@task, @reply]) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'fields', f: f %>
      <%= f.submit "Criar", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>